                pbuf$(1)=bmm05a.SUBCONT_SEQ$+" "+bmm05a.LINE_TYPE$+" "+fnmask$(bmm05a.VENDOR_ID$(1,vlen),vmask$)+" "+apm01a.vendor_name$(1,25-len(vmask$))
