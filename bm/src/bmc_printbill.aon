                pbuf$(47)=str(bmm03a.PCS_PER_HOUR:bms01a.MAT_FAC_MSK$)
                pbuf$(67)=str(100*(bmm03a.HRS_PER_PCE/bmm03a.PCS_PER_HOUR)/yield+bmm03a.SETUP_TIME/lot:bms01a.HRQTY_MASK$)
                if wo$="Y" then 
                    pbuf$(19,len(str(bmm08a.QUEUE_TIME:m6$)))=str(bmm08a.QUEUE_TIME:m6$)
                    pbuf$(59,len(str(bmm03a.MOVE_TIME:m6$)))=str(bmm03a.MOVE_TIME:m6$)
