            if cvs(bmm03a.OBSOLT_DATE$,3)<>"" then if bmm03a.OBSOLT_DATE$<=pdate$ then continue
            endif
        endif
        if bmm03a.LINE_TYPE$="M" then
            dim pbuf$(pbufwidth)
                pbuf$(1)=bmm03a.OP_SEQ$
