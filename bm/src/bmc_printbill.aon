                pbuf$(51)=str(bmm05a.ALT_FACTOR:bms01a.MAT_FAC_MSK$)
                pbuf$(60)=str(bmm05a.UNIT_COST:ivs01a.PRICE_MASK$)
                pbuf$(69)=str(bmm05a.UNIT_COST*fnsub(bmm05a.QTY_REQUIRED,bmm05a.ALT_FACTOR,bmm05a.DIVISOR):ivs01a.PRICE_MASK$)
