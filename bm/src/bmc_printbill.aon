            OutVect!.addItem(pbuf$)
        else 
            let bmm08a.CODE_DESC$="Not On File"
            bmm08a.DIRECT_RATE=0
            read record (bmm08_dev,key=bmm03a.FIRM_ID$+bmm03a.OP_CODE$,dom=*next) bmm08a$
