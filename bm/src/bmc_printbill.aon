                pbuf$(38)="Ld: "+str(bmm05a.LEAD_TIME:p4$)
                pbuf$(47)="U/M: "+bmm05a.UNIT_MEASURE$
                pbuf$(55)="Op: "+bmm05a.OP_SEQ$
