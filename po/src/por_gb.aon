rem         k01$=key(poe01_dev,end=nextalt)
rem         if k01$(1,8)<>k04b$ goto nextalt
            k01$=key(poe01_dev,end=keybyalt)
            if k01$(1,8)<>k04b$ goto keybyalt
            k$=k01$
