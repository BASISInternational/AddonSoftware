testkey:
    if k$(1,2)<>firm_id$ goto done
    read record (poe01_dev,key=k$) poe01a$

rem --- Test Ranges

    headrecprinted$="N"
    if reqfrom$<>"" and a0$(9,7)<reqfrom$ goto nextheader
    if reqthru$<>"" and a0$(9,7)>reqthru$ goto nextheader
    if printseq$="A" if vendfrom$<>"" if cvs(k04a$(4,10),2)<cvs(vendfrom$,2) goto nextheader
    if printseq$="A" if vendthru$<>"" if cvs(k04a$(4,10),2)>cvs(vendthru$,2) goto nextheader
rem if printseq$="A" goto l1760
    if printseq$="A" goto brnch1
    if vendfrom$<>"" and a0$(3,6)<vendfrom$ goto nextheader
    if vendthru$<>"" and a0$(3,6)>vendthru$ goto nextheader
