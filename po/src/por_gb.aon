    :                   @(43),fnmask$(b2$(11,custlen),customask$)," Order ",b2$(17,7)," ",b2$(24,3),
    :                   @(73),"Ship To:",
    :                   @(82),fnmask$(b2$(27,custlen),customask$)
    print (printer_dev) @(38),"Name",
