brnch2:
rem find record (arm03_dev,key=b0$(1,2)+b2$(11,6)+b2$(27,6),dom=l4880) arm03a$
    find record (arm03_dev,key=b0$(1,2)+b2$(11,6)+b2$(27,6),dom=brnch3) arm03a$
    dropname$=dropinfo$(1,30)
    dropaddr$=dropinfo$(31,72)+dropinfo$(127,24)+dropinfo$(151,9)
    call pgmdir$+"adc_address.aon",dropaddr$,24,4,9,30
