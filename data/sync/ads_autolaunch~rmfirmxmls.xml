<barista_admin_record>
	<barista_record_table>ADS_AUTOLAUNCH</barista_record_table>
	<PROCESS_ID>RMFIRMXMLS</PROCESS_ID>
	<DESCRIPTION>Remove non-firm 99 sync XMLs</DESCRIPTION>
	<PROGRAM_PATH>[+DIR_PGM]adx_rmfirmxmls.aon</PROGRAM_PATH>
	<PROCESS_TYPE>O</PROCESS_TYPE>
	<PROCESS_STATUS>N</PROCESS_STATUS>
	<ASC_COMP_ID>01007514</ASC_COMP_ID>
	<ASC_PROD_ID>AD </ASC_PROD_ID>
	<COMMENTS>This progarm removes non-firm 99 ads_masks~*.xml and ads_sequences~*.xml files in aon/data/sync directory. This ensures masks and sequences will be backed up during a sync backup. Only demo data masks and sequences are available via the download. So for an upgrade, a firm's masks and sequences must be in the sync backup or else they'll be lost.[$0a$]                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     </COMMENTS>
</barista_admin_record>
