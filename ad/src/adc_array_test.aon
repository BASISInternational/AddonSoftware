rem --- adc_array_test.aon

    use ::adc_array.aon::Obj4Array
    
    declare Obj4Array in!
    declare Obj4Array out!

    test = 3
    
    switch test
        case 1
            dim a$[3]
            a$[0] = "apple"
            a$[1] = "orange"
            a$[2] = "pear"
            break
        case 2
            dim a$[3,2]
            a$[0,0] = "CA"
            a$[0,1] = "San Francisco"
            a$[0,2] = "Los Angelos"
            a$[1,0] = "OR"
            a$[1,1] = "Portland"
            a$[1,2] = "Medford"
            a$[2,0] = "WA"
            a$[2,1] = "Vancouver"
            break
        case 3
            dim a$[2,2,2]
            a$[0,0,0]="The Beatles"
            
            a$[0,1,0]="Abbey Road"
            a$[0,1,1]="Come Together"
            a$[0,1,2]="Maxwell's Silver Hammer"
            
            a$[0,2,0]="The White Album"
            a$[0,2,1]="Back in the USSR"
            a$[0,2,2]="Helter Skelter"
            
            a$[1,0,0]="Sting"
            
            a$[1,1,0]="Soul Cages"
            a$[1,1,1]="All This Time"
            a$[1,1,2]="The Ship of the World"
            
            a$[1,2,0]="Ten Sumner Tales"
            a$[1,2,1]="Love is Thicker than Blood"
            
            a$[2,0,0]="Joni Mitchel"
            
            a$[2,1,0]="Court and Spark"
            a$[2,1,1]="Help Me"
            a$[2,1,2]="Troubled Child"
            
            a$[2,2,0]="Blue"
            a$[2,2,1]="California"
            a$[2,2,2]="I'm a Radio"
            break
        case default
    swend
    
    call "adc_array.aon::str_array2object", a$[all], out!, status
    if status = 999 then escape
    
    in! = out!
    
    call "adc_array.aon::str_object2array", in!, b$[all], status
    if status = 999 then escape
    
    call "/devel/utils/dump_array", b$[all]

end
