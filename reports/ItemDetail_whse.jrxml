	<query language="sql"><![CDATA[select warehouse_id, item_id, location, pi_cyclecode, ar_dist_code, select_phys, buyer_code, vendor_id, cur_price_cd, pri_price_cd, abc_code, eoq_code, ord_pnt_code, saf_stk_code, special_ord, qty_on_hand, qty_on_order, qty_commit, maximum_qty, order_point, eoq, safety_stock, lead_time, physical_cnt, cur_price, pri_price, unit_cost, avg_cost, std_cost, rep_cost, landed_cost, last_po_cost, lstphy_date, arc_distcode.code_desc AS dist_desc, ivc_whsecode.short_name AS whse_name, ivc_buycode.code_desc AS buyer_name, apm_vendmast.vendor_name AS vend_name from ivm_itemwhse
left join arc_distcode on arc_distcode.firm_id = $P{FIRM_ID} and arc_distcode.record_id_d = 'D' and arc_distcode.ar_dist_code = ivm_itemwhse.ar_dist_code
left join ivc_whsecode on ivc_whsecode.firm_id = $P{FIRM_ID} and ivc_whsecode.record_id_c = 'C' and ivc_whsecode.warehouse_id = ivm_itemwhse.warehouse_id
left join ivc_buycode on ivc_buycode.firm_id = $P{FIRM_ID} and ivc_buycode.record_id_f = 'F' and ivc_buycode.buyer_code = ivm_itemwhse.buyer_code
left join apm_vendmast on apm_vendmast.firm_id = $P{FIRM_ID} and apm_vendmast.vendor_id = ivm_itemwhse.vendor_id
where firm_id = $P{FIRM_ID} and item_id = $P{ITEM_ID}
order by firm_id, item_id, warehouse_id]]></query>
