	<parameter name="FIRM_ID" class="java.lang.String"/>
	<parameter name="CUSTOMER_ID" class="java.lang.String"/>
	<query language="sql"><![CDATA[select firm_id, customer_id, shipto_no, name, addr_line_1, addr_line_2, addr_line_3, addr_line_4, city, state_code, zip_code, phone_no, phone_exten, contact_name, slspsn_code, territory, tax_code, db_no, sic_code, country, arc_salecode.code_desc AS sales_desc, arc_terrcode.code_desc AS terr_desc, opc_taxcode.code_desc AS tax_desc,ar_ship_via,shipping_id,shipping_email
from arm_custship
left join arc_salecode on firm_id = arc_salecode.firm_id and arc_salecode.record_id_f = 'F' and slspsn_code = arc_salecode.slspsn_code
left join arc_terrcode on firm_id = arc_terrcode.firm_id and arc_terrcode.record_id_h = 'H' and territory = arc_terrcode.territory
left join opc_taxcode on firm_id = opc_taxcode.firm_id and tax_code = opc_taxcode.op_tax_code
where firm_id = $P{FIRM_ID} and customer_id = $P{CUSTOMER_ID}]]></query>
